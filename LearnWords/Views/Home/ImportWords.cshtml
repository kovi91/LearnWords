@model string

@{
    ViewData["Title"] = "ImportWords";
}

<h2>Import Words</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        @using (Html.BeginForm("ImportWords", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <div class="form-group">
                <label for="Picture">Zip</label>
                <input type="file" class="form-control-file" id="Zip" name="Zip">
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
            <input type="hidden" value="@Model" name="categoryhash" id="categoryhash" />
        }
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}


